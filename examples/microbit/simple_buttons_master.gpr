--  GNAT Project file for the Simple_Buttons demo.
--  This aggregate project sets up options for the main GPR.

aggregate project Simple_Buttons_Master is

   for Target use "arm-eabi";

   for Runtime ("ada") use
     external ("HOME") & "/cortex-gnat-rts/local/microbit";
   for external ("RTS") use "microbit";

   for external ("BUILD") use "Production";

   for Project_Files use ("simple_buttons.gpr");
   for Project_Path use ("../..");

end Simple_Buttons_Master;
