<!ELEMENT association ( name, role+, associative ) >

<!ELEMENT associative ( #PCDATA ) >

<!ELEMENT attribute ( name, documentation, type ) >
<!ATTLIST attribute stereotype NMTOKEN #IMPLIED >

<!ELEMENT attributes ( attribute* ) >

<!ELEMENT cardinality ( #PCDATA ) >

<!ELEMENT child ( #PCDATA ) >

<!ELEMENT classname ( #PCDATA ) >

<!ELEMENT documentation ( #PCDATA ) >

<!ELEMENT domain ( name, objects, relationships ) >

