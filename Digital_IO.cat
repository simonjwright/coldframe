(object Class_Category "Digital IO"
    is_unit    	TRUE
    is_loaded  	TRUE
    quid       	"3DFCF76B0011"
    documentation 	
|This domain manages digital (discrete) input/output.
|It is intended for simulations only, so there is no actual digital I/.O, only support for an HCI.
|
|[[ revision : $Id: Digital_IO.cat,v 5bb69c85c5ad 2002/12/28 17:28:48 simon $ ]]
    
    exportControl 	"Public"
    logical_models 	(list unit_reference_list
	(object Class "Signal"
