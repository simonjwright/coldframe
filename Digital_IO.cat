		    opExportControl 	"Public"
		    uid        	0)
		(object Operation "set state"
		    quid       	"3DFF69A503E3"
		    documentation 	"Set the signal's state as indicated; call Changed if it has changed."
		    parameters 	(list Parameters
			(object Parameter "to"
			    type       	"Boolean"))
		    concurrency 	"Sequential"
		    opExportControl 	"Public"
		    uid        	0)
		(object Operation "get state"
		    quid       	"3DFF69B600EF"
		    stereotype 	"accessor"
		    result     	"Boolean"
		    concurrency 	"Sequential"
		    opExportControl 	"Public"
		    uid        	0))
	    class_attributes 	(list class_attribute_list
		(object ClassAttribute "s"
		    quid       	"3DFCF7A9029B"
		    stereotype 	"id"
		    type       	"Signal Name")
